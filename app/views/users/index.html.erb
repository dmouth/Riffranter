<table cellpadding="5" cellspacing="0" class="index-table">
	<tr>
		<th valign="top"><%= User.human_attribute_name :first_name %></th>
		<th valign="top"><%= User.human_attribute_name :last_name %></th>
		<th valign="top"><%= User.human_attribute_name :email %></th>
		<th valign="top"><%= User.human_attribute_name :created_at %></th>
		<th valign="top"><%= User.human_attribute_name :updated_at %></th>
<th></th>
	</tr>
	<% @users.each do |user| %>
	<tr class="<%= cycle "odd-row", "even-row" %>">
		<td valign="top"><%= user.first_name %></td>
		<td valign="top"><%= user.last_name %></td>
		<td valign="top"><%= user.email %></td>
		<td valign="top"><%= user.created_at %></td>
		<td valign="top"><%= user.updated_at %></td>
	<td valign="top">
		<%= link_to "Show", user %> |
		<%= link_to "Edit", edit_user_url(user) %> |
		<%= link_to "Destroy", user, :confirm => 'Are you sure?', :method => :delete %>
	</td>
	</tr>
	<% end %>
</table>
